---
layout: base.njk
---

<div id = "board_members">
  <h1> Meet the board members... </h1>

  {% for member in collections.member %}
  <div>
    <img src="{{member.data.photo}}">
    <div>
      <h4>{{member.data.name}}</h4>
      <h5>{{member.data.title}}</h5>
      <p> {{member.templateContent | safe}} </p>
    </div>
  </div>
  <br>
  <br>
  {% endfor %}

</div>
